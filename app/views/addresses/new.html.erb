<% provide(:title, '住所の追加') %>
<h1>住所の追加</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@address) do |f| %>
      <%= render 'shared/error_messages', object: f.object %>
      <%= f.label :address %>
      <%= f.text_field :address, class: 'form-control' %>

      <%= f.submit "住所を登録する", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
